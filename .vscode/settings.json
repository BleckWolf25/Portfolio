{
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"eslint.validate": [
		"javascript",
		"json",
		"typescript",
	],
	"editor.codeActionsOnSave": {
		"source.fixAll.ts": "always",
		"source.fixAll.eslint": "always"
	},
	"[javascript]": {
		"editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
	}
}
